{
    "name": "nuwber/hypervel-gelf-logger",
    "description": "A Hypervel package to send logs to a gelf compatible backend like graylog",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "nuwber",
        "hypervel",
        "hypervel-gelf",
        "gelf",
        "udp",
        "log",
        "logging",
        "graylog",
        "graylog2",
        "php"
    ],
    "authors": [
        {
            "name": "hedii",
            "homepage": "https://hedichaibi.com"
        }
    ],
    "support": {
        "issues": "https://github.com/hedii/laravel-gelf-logger/issues",
        "source": "https://github.com/hedii/laravel-gelf-logger"
    },
    "require": {
        "php": "^8.2",
        "graylog2/gelf-php": "^2.0",
        "hypervel/log": "^0.1"
    },
    "require-dev": {
        "hyperf/testing": "^3.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nuwber\\HypervelGelfLogger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nuwber\\HypervelGelfLogger\\Tests\\": "tests/"
        }
    },
    "extra": {
        "hyperf": {
            "config": "Nuwber\\HypervelGelfLogger\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
